/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Nand(a=true, b=a, out=out1);
    Nand(a=true, b=b, out=out2);
    Nand(a=out1, b=out2, out=out3);
    Nand(a=a, b=b, out=out4);
    Nand(a=out3, b=out4, out=out5);
    Nand(a=true, b=out5, out=out);
}